# 기존 데이터 삭제
$data remove entity @n[tag=nomic.ui_$(id), type=minecraft:chest_minecart] Items

# 버튼 설정
$data modify entity @n[tag=nomic.ui_$(id), type=minecraft:chest_minecart] Items append value {Slot: 0b, id: "minecraft:barrier", components: {"minecraft:custom_data": {nomic.button: {cmd: ""}}, "minecraft:custom_model_data": 1, "minecraft:custom_name": '{"text": "홈 및 최근 소리", "color": "white", "italic": false}'}}
$data modify entity @n[tag=nomic.ui_$(id), type=minecraft:chest_minecart] Items append value {Slot: 1b, id: "minecraft:barrier", components: {"minecraft:custom_data": {nomic.button: {cmd: "data modify storage nomic:global data[$(id)].tab set value 1"}}, "minecraft:custom_model_data": 0, "minecraft:custom_name": '{"text": "마인크래프트", "color": "white", "italic": false}'}}
$data modify entity @n[tag=nomic.ui_$(id), type=minecraft:chest_minecart] Items append value {Slot: 2b, id: "minecraft:barrier", components: {"minecraft:custom_data": {nomic.button: {cmd: "data modify storage nomic:global data[$(id)].tab set value 2"}}, "minecraft:custom_model_data": 0, "minecraft:custom_name": '{"text": "대화", "color": "white", "italic": false}'}}

